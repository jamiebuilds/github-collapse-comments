{
  "name": "Github Collapse Comments",
  "version": null,
  "description": "Collapse block comments from source viewer",
  "homepage_url": "https://github.com/thejameskyle/github-collapse-comments",
  "manifest_version": 2,
  "minimum_chrome_version": "36",
  "icons": {
  },
  "permissions": [
    "https://github.com/*/*"
  ],
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [
        "https://github.com/*/*"
      ],
      "css": [
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
